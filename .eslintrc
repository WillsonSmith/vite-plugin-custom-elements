{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:wc/recommended",
    "plugin:lit/recommended",
    "plugin:lit-a11y/recommended",
    "prettier",
    "plugin:storybook/recommended",
    "plugin:storybook/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "lit-a11y"
  ],
  "ignorePatterns": [
    "node_modules/",
    "lib/",
    "dist/",
    "test/**/*.test.js"
  ]
}